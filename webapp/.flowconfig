[ignore]
.*/public/.*
.*/config/.*
.*/scripts/.*
.*/tests/.*
.*/node_modules/.*
.*/registerServiceWorker.js
.*/src/index.js

[include]
.*/src/**/*.js
.*/src/**/*.json

[libs]

[lints]

[options]
module.name_mapper='^components/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^containers/\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^@/\(.*\)$' -> '<PROJECT_ROOT>/src/helpers/\1'

[strict]
